{
  "listeners": {
    "*:8080": {
      "pass": "applications/app"
    }
  },
  "applications": {
    "app": {
      "type": "python",
      "path": "/app/src",
      "module": "app",
      "callable": "app"
    }
  },
  "access_log": {
    "path": "/var/log/unit/access.log",
    "format": "$remote_addr - - [$time_local] \"$request_line\" $status $body_bytes_sent \"$header_referer\" \"$header_user_agent\" $request_time"
  }
}