{
  "listeners": {
    "*:8080": {
      "pass": "routes"
    }
  },

  "routes": [
    {
      "match": {
        "uri": "/*"
      },
      "action": {
        "share": "/static$uri",
        "fallback": {
          "pass": "applications/app"
        }
      }
    }
  ],

  "applications": {
    "app": {
      "type": "python",
      "path": "/app/src",
      "module": "app",
      "callable": "app"
    }
  }
}
